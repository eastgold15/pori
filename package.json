{
	"name": "robino",
	"version": "0.0.16",
	"description": "modules",
	"type": "module",
	"main": "./mod.js",
	"types": "./mod.d.ts",
	"exports": {
		".": {
			"types": "./mod.d.ts",
			"main": "./mod.js"
		},
		"./config/prettier": {
			"types": "./config/prettier/mod.d.ts",
			"default": "./config/prettier/mod.js"
		},
		"./util/md": {
			"types": "./util/md/mod.d.ts",
			"default": "./util/md/mod.js"
		}
	},
	"scripts": {
		"dev": "tsc --project tsconfig.build.json --watch",
		"clean": "rm -rf ./dist",
		"copy": "cp package.json ./dist",
		"build": "bun clean && tsc --project tsconfig.build.json && bun copy",
		"format": "prettier --write .",
		"pub": "bun i && bun run build && npm publish --access public --no-git-checks ./dist"
	},
	"author": {
		"name": "Ross Robino",
		"url": "https://robino.dev"
	},
	"repository": "github:rossrobino/mod",
	"license": "MIT",
	"dependencies": {
		"@types/js-yaml": "^4.0.5",
		"highlight.js": "^11.8.0",
		"js-yaml": "^4.1.0",
		"marked": "^9.0.0",
		"marked-gfm-heading-id": "^3.0.8",
		"marked-highlight": "^2.0.6",
		"marked-smartypants": "^1.1.3",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@types/node": "^20.6.1",
		"bun-types": "latest",
		"prettier": "^3.0.3",
		"robino": "^0.0.14",
		"typescript": "^5.2.2"
	}
}
