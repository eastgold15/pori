{
	"name": "robino",
	"version": "0.0.15",
	"description": "modules",
	"type": "module",
	"main": "./mod.js",
	"types": "./mod.d.ts",
	"exports": {
		".": {
			"types": "./mod.d.ts",
			"main": "./mod.js"
		},
		"./config/prettier": {
			"types": "./config/prettier/mod.d.ts",
			"default": "./config/prettier/mod.js"
		}
	},
	"scripts": {
		"dev": "tsc --project tsconfig.build.json --watch",
		"clean": "rm -rf ./dist",
		"copy": "cp package.json ./dist",
		"build": "pnpm clean && tsc --project tsconfig.build.json && pnpm copy",
		"format": "prettier --write .",
		"pub": "pnpm i && pnpm build && pnpm publish --access public --no-git-checks ./dist"
	},
	"author": {
		"name": "Ross Robino",
		"url": "https://robino.dev"
	},
	"repository": "github:rossrobino/mod",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.6.1",
		"prettier": "^3.0.3",
		"robino": "^0.0.14",
		"typescript": "^5.2.2"
	}
}
