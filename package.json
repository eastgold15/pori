{
	"name": "robino",
	"version": "0.0.30",
	"description": "modules",
	"type": "module",
	"types": "./dist/types/index.d.ts",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"default": "./dist/index.js"
		},
		"./config/prettier": {
			"types": "./dist/types/index.d.ts",
			"default": "./dist/config/prettier/index.js"
		},
		"./util/md": {
			"types": "./dist/types/index.d.ts",
			"default": "./dist/util/md/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsc --project tsconfig.build.json --watch",
		"format": "prettier --write .",
		"build:clean": "rm -rf ./dist",
		"build:js": "tsc --project tsconfig.build.json",
		"build:dts": "bun scripts/dts/index.ts",
		"build": "bun build:clean && bun build:js && bun build:dts",
		"pub:only": "npm publish --access public",
		"pub": "bun i && bun format && bun run build && bun pub:only"
	},
	"author": {
		"name": "Ross Robino",
		"url": "https://robino.dev"
	},
	"repository": "github:rossrobino/mod",
	"license": "MIT",
	"dependencies": {
		"highlight.js": "^11.8.0",
		"js-yaml": "^4.1.0",
		"marked": "^9.0.1",
		"marked-gfm-heading-id": "^3.0.8",
		"marked-highlight": "^2.0.6",
		"marked-smartypants": "^1.1.3",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.5",
		"bun-types": "latest",
		"prettier": "^3.0.3",
		"robino": "^0.0.19",
		"typescript": "^5.2.2"
	}
}
